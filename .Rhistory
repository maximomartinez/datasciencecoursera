complete <- function(directory, id = 1:332) {
x <- numeric(0)
y <- numeric(0)
totalfiles <- list.files(directory, full.names = TRUE)
for(i in id){
x[i] <- i
y[i] <- nrow(na.omit(read.csv(totalfiles[i])))
}
data <- data.frame(id=x, nobs=y)
comp <- na.omit(data)
rownames(comp) <- 1:nrow(comp)
return(comp)
}
